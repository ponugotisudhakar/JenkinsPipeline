node('Anible'){
   
    stage('SCM'){
        git 'https://github.com/spring-projects/spring-petclinic.git'

    }
    stage('mvn pakcage'){
        sh 'mvn pakcage'

    }
     stage('test results'){
        junit 'target/surefire-reports/*.xml'

    }
    stage('archive'){
        archive 'target/*.jar'

    }
}